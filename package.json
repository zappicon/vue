{
  "name": "@zappicon/vue",
  "version": "0.0.4",
  "description": "Free & premium UI icons library, crafted to simplify workflows for designers, developers and creators.",
  "main": "dist/index.mjs",
  "browser": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "icons",
    "vue",
    "svg",
    "components",
    "zappicon"
  ],
  "author": {
    "name": "Haroun Abidi",
    "email": "contact@harounabidi.com",
    "url": "https://harounabidi.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappicon/vue.git"
  },
  "bugs": {
    "url": "https://github.com/zappicon/vue/issues"
  },
  "homepage": "https://zappicon.com",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist && rimraf src/icons && rimraf src/variants && rimraf src/index.ts",
    "generate": "npm run clean && tsx scripts/generate.ts",
    "build": "npm run generate && vite build && vue-tsc",
    "test": "vitest --watch=false",
    "test:coverage": "vitest --coverage --watch=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.20",
    "vue-tsc": "^3.0.6"
  }
}
